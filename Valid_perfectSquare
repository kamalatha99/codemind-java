import java.util.*;
class Perfect_Square
{
    static void isPerfect(int num)
    {
        int fl=0;
        for(int i=1;i<num;i++)
        {
            if(num==i*i)
            {
                fl=1;
                System.out.println("True");
            }
        }
        if(fl==0)
        {
            System.out.println("False");
        }
    }
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        int[] a=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=obj.nextInt();
        }
        for(int i=0;i<n;i++)
        {
            isPerfect(a[i]);
        }
    }
}
