import java.util.*;
class PRO_SUM
{
    static void fun(int num)
    {
        int p=1;
        int s=0;
        while(num!=0)
        {
            int r=num%10;
            p=p*r;
            s=s+r;
            num=num/10;
        }
        System.out.println(p-s);
    }
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        fun(n);
    }
}
