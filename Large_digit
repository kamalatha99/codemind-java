import java.util.*;
class Large_Digit
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        int max=1;
        while(n!=0)
        {
            int r=n%10;
            if(r>max)
            {
                max=r;
            }
            n=n/10;
        }
        System.out.println(max);
    }
}
