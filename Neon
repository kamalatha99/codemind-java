import java.util.*;
class Neon
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        int s=n*n;
        int sum=0;
        while(s!=0)
        {
            int r=s%10;
            sum=sum+r;
            s=s/10;
        }
        if(sum==n)
        {
            System.out.println("Neon Number");
        }
        else
        {
            System.out.println("Not Neon Number");
        }
    }
}
