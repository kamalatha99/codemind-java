import java.util.*;
class Palin
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        int rev=0;
        int t=n;
        while(n!=0)
        {
            int r=n%10;
            n=n/10;
            rev=rev*10+r;
        }
        if(t==rev)
        {
            System.out.println("True");
        }
        else
        {
            System.out.println("False");
        }
    }
}
