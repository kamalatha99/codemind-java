import java.util.*;
class Countdivisors_range
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int m=obj.nextInt();
        int n=obj.nextInt();
        int k=obj.nextInt();
        int c=0;
        for(int i=m;i<=n;i++)
        {
            if(i%k==0)
            {
                c++;
            }
        }
        System.out.println(c);
    }
}
