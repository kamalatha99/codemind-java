import java.util.*;
class Harshed
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        int t=n;
        int sum=0;
        while(n!=0)
        {
            int r=n%10;
            sum=sum+r;
            n=n/10;
        }
        if(t%sum==0)
        {
            System.out.println("True");
        }
        else
        {
            System.out.println("False");
        }
    }
}
