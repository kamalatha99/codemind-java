import java.util.*;
class sumk
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        int[] a=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=obj.nextInt();
        }
        int k=obj.nextInt();
        int sum=0;
        for(int i=0;i<k;i++)
        {
            sum=sum+a[i];
        }
        System.out.println(sum);
    }
}
