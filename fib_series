import java.util.*;
class Fibbanoci
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
            int f=0;
            int s=1;
            System.out.print(f+" "+s+" ");
            int c=2;
       while(c<n)
       {
           int k=f+s;
           System.out.print(k+" ");
           f=s;
           s=k;
           c++;
       }
        
    }
}
