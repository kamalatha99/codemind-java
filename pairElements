import java.util.*;
class Pair
{
    public static void main(String args[])
    {
        Scanner obj=new Scanner(System.in);
        int n=obj.nextInt();
        ArrayList<Integer> al=new ArrayList<Integer>(n);
        for(int i=0;i<n;i++)
        {
            al.add(obj.nextInt());
        }
        if(n%2!=0)
        {
            al.add(0);
        }
        for(int i=0;i<al.size();i++)
        {
            System.out.print(al.get(i)+" ");
        }
    }
}
